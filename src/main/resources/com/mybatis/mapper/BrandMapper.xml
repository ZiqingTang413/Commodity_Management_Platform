<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatis.mapper.BrandMapper">
    <resultMap id="brandResultMap" type="com.mybatis.pojo.Brand">
        <result column="brand_name" property="brandName" />
        <result column="company_name" property="brandName" />
    </resultMap>


    <select id="selectAll" parameterType="int" resultMap="brandResultMap">
        select *
        from tb_brand;
    </select>

    <select id="selectById" resultMap="brandResultMap">
        select *
        from tb_brand where id = #{id};
    </select>


</mapper>